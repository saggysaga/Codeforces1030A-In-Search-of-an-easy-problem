#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,a=0;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
   
    {
        cin>>arr[i];
        if(arr[i]!=0)
        {
            a++;
        }
    }
    if(a>0){
        cout<<"HARD";
    }
    else{
        cout<<"EASY";
    } 
}
